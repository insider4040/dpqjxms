#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <conio.h>
#include <math.h>
//열거체 사용해야 함

typedef struct Song {
	char title[100];
	char artist[100];
	char genre[100];
}song_t;

int main()
{
	song_t song[8] = {
		{"밤편지", "아이유","ballad"},
		{"I'm The One", "DJ Khaled","hip-hop"},
		{"Jealous", "DJ Khaled","hip-hop"},
		{"한여름밤의 꿀", "San E","hip-hop"},
		{"서울 밤", "어반자카파","soul"},
		{"썸머", "GRAY","soul"},
		{"누구 없소(No One)", "이하이","dance"},
		{"Sixteen", "Ellie Goulding","pop"},
	};

	//문자열이니까 %s로 해야지 ^^
	for (int i = 0; i < 8; ++i) {
		printf("%-5s %20s %20s\n", song[i].title, song[i].artist, song[i].genre);
	}
}
